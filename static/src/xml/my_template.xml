<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-name="estate_molhimah.MyTemplate">
        <div class="o_custom_highlight">
            <strong>Hello from custom QWeb Template!</strong>
        </div>
    </t>
    <t t-name="estate_molhimah.PropertyList">
        <div class="o_property_list">
            <h2>قائمة العقارات</h2>
            <t t-foreach="properties" t-as="property">
                <div class="o_property_item">
                    <strong><t t-esc="property.name"/></strong>
                    <p><t t-esc="property.address"/></p>
                </div>
            </t>
        </div>
    </t>
</templates>
