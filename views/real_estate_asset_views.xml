<odoo>
    <record id="view_real_estate_asset_list" model="ir.ui.view">
        <field name="name">real.estate.asset.list</field>
        <field name="model">real.estate.asset</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
                <field name="property_type"/>
                <field name="status"/>
                <field name="price"/>
                <field name="owner_id"/>
            </list>
        </field>
    </record>

    <record id="view_real_estate_asset_form" model="ir.ui.view">
        <field name="name">real.estate.asset.form</field>
        <field name="model">real.estate.asset</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="description"/>
                        <field name="price"/>
                        <field name="area"/>
                        <field name="location"/>
                        <field name="property_type"/>
                        <field name="status"/>
                        <field name="owner_id"/>
                        <field name="image"/>
                    </group>
                
                </sheet>
            </form>
        </field>
    </record>

    <record id="action_real_estate_asset" model="ir.actions.act_window">
        <field name="name">العقارات</field>
        <field name="res_model">real.estate.asset</field>
        <field name="view_mode">list,form</field>
        <field name="view_id" ref="view_real_estate_asset_list"/>
    </record>
</odoo>
