<odoo>
    <record id="view_project_list" model="ir.ui.view">
        <field name="name">real.estate.project.list</field>
        <field name="model">real.estate.project</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
                <field name="location"/>
            </list>
        </field>
    </record>
    <record id="view_project_form" model="ir.ui.view">
        <field name="name">real.estate.project.form</field>
        <field name="model">real.estate.project</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="description"/>
                        <field name="location"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="action_real_estate_project" model="ir.actions.act_window">
        <field name="name">المشاريع العقارية</field>
        <field name="res_model">real.estate.project</field>
        <field name="view_mode">list,form</field>
        <field name="view_id" ref="view_project_list"/>
    </record>
</odoo>
